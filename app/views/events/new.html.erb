<h2>Create Event!</h2>

<%= form_with model: @event do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :location %>
  <%= form.text_field :location %>
  <%= form.label :start_time %>
  <%= form.datetime_field :start_time %>
  <%= form.label :end_time %>
  <%= form.datetime_field :end_time %>
  <%= form.submit "Create" %>
<% end %>